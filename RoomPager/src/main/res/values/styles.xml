<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoomPagerStyle">
        <attr name="isNextRoomLoadable" format="boolean" />
        <attr name="roomPosition" format="integer" />
        <attr name="pageThreshold" format="float" />
        <attr name="gridSize" format="integer" />
        <attr name="orientation" format="enum">
            <enum name="vertical" value="0"/>
            <enum name="horizontal" value="1"/>
            <enum name="both" value="2"/>
        </attr>
    </declare-styleable>
</resources>
